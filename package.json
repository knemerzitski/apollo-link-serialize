{
  "name": "@knemerzitski/apollo-link-serialize",
  "version": "5.1.1",
  "versionUpstream": "4.0.0",
  "description": "A link that serializes requests by key, making sure that they execute in the exact order submitted",
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "zen-observable-ts": "^1.2.5"
  },
  "devDependencies": {
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@vitest/coverage-v8": "^2.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "graphql": "^16.9.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.2"
  },
  "main": "build/dist/index.js",
  "module": "build/dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "knemerzitski/apollo-link-serialize"
  },
  "jsnext:main": "build/dist/index.js",
  "typings": "build/dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --d build/dist --format cjs,esm --dts --clean",
    "lint": "eslint -c .eslintrc.json --ext .ts src/**/*",
    "test": "vitest --environment=jsdom",
    "coverage": "vitest --coverage"
  },
  "keywords": [
    "graphql",
    "apollo",
    "apollo-link",
    "serialize",
    "queue"
  ],
  "author": "Jonas Helfer <jonas@helfer.email>",
  "license": "MIT"
}
